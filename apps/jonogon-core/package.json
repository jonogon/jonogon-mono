{
  "name": "@jonogon/core",
  "version": "0.0.1",
  "description": "The main REST/TRPC API for jonogon-web.",
  "main": "./dist/index.mjs",
  "type": "module",
  "scripts": {
    "start": "node .",
    "start:watch": "linkemon --inspect=0.0.0.0:14001 .",
    "build": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "dev": "concurrently pnpm:build:watch pnpm:start:watch",
    "kysely:codegen": "kysely-codegen --out-file ./src/db/postgres/types.mts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.629.0",
    "@t3-oss/env-core": "0.11.0",
    "@trpc/server": "10.45.2",
    "bufferutil": "4.0.8",
    "cors": "2.8.5",
    "es-toolkit": "1.15.1",
    "express": "4.19.2",
    "hasha": "6.0.0",
    "ioredis": "5.4.1",
    "jsonwebtoken": "9.0.2",
    "kysely": "0.27.4",
    "nanoid": "^5.0.7",
    "pg": "8.12.0",
    "scope-utilities": "1.2.2",
    "sharp": "^0.33.4",
    "winston": "3.14.0",
    "ws": "8.18.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "22.1.0",
    "@types/pg": "8.11.6",
    "@types/ws": "8.5.12",
    "concurrently": "8.2.2",
    "kysely-codegen": "0.15.0",
    "linkemon": "0.2.0",
    "nodemon": "2.0.22",
    "trpc-playground": "1.0.4",
    "typescript": "5.5.4"
  },
  "license": "MIT",
  "volta": {
    "node": "22.6.0"
  }
}
